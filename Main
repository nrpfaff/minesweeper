import javax.swing.SwingUtilities;

public class Main {

	public static void main(String[] args) {
		
		GameBoard board = new GameBoard();
		
		board.clearBoard();
		board.placeBombs();
		board.bombCounter();
		
			SwingUtilities.invokeLater(new Runnable(){
			public void run() {
				new GameMenu(board);
			}
		});	
	}
}
